<launch> 
  <node type="cameracalibrator.py" pkg="camera_calibration"
        args="--size=7x6 --square=0.108 --camera_name=prosilica_kinect"
        name="calibration_gui" output="screen">
    <remap from="image" to="prosilica_kinect/image_raw" />
